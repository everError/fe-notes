{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@vue-editor/core": ["packages/core/src"],
      "@vue-editor/schema": ["packages/schema/src"]
    },
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["packages/**/*", "apps/**/*"]
}
